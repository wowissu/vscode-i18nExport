{
  "name": "i18n-export",
  "displayName": "i18nExport",
  "publisher": "i18nExport",
  "description": "export i18n json file",
  "version": "0.0.4",
  "icon": "img/icon-v1.jpg",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wowissu/vscode-i18nExport"
  },
  "activationEvents": [
    "onCommand:extension.i18nExport"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.i18nExport",
        "title": "i18n to excel"
      }
    ],
    "configuration": {
      "title": "i18nExport",
      "properties": {
        "i18nExport.localesPath": {
          "type": "string",
          "default": "src/i18n",
          "description": "Where i18n json files stored."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "json2xls": "^0.1.2"
  }
}
